{

  "name": "Vim what?",
  "description": "Lookup of nifty vim commands",
  "version": "0.1",
	"manifest_version": 2,

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
		"default_title": "Vim what?"
  },

	"background": {
		"scripts" : ["/lib/jquery-2.0.3.min.js"]
	},

  "permissions": [
    "activeTab",
		"\u003Call_urls\u003E",
		"contextMenus",
		"notifications",
    "tabs",
    "cookies",
    "unlimitedStorage",
    "storage"
  ],

  "icons": {
    "32": "icon-32.jpg",
    "128": "icon-128.jpg"
  }
}
